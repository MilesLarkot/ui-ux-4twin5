{
  "name": "ui-ux-4twin5",
  "version": "1.0.0",
  "main": "tailwind.config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:css": "npm run build:tailwind && npm run build:sass",
    "build:tailwind": "tailwindcss -i ./src/style.css -o ./public/style.css --minify",
    "build:sass": "sass ./src/sass/main.scss ./public/main.css --style=compressed",
    "watch:css": "npm run watch:tailwind & npm run watch:sass",
    "watch:tailwind": "tailwindcss -i ./src/style.css -o ./public/style.css --watch",
    "watch:sass": "sass ./src/sass/main.scss ./public/main.css --watch" 
  },
